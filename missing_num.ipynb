   "source": [
    "# Find the Missing Number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "def miss_num(arr, n):\n",
    "    t_sum = n*(n+1)//2\n",
    "    s = 0\n",
    "    for i in arr:\n",
    "        s += i\n",
    "    return t_sum - s"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [
