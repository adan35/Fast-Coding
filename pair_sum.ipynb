   "source": [
    "def pair_sum(arr, k):\n",
    "    hashmap = {}\n",
    "    for i in range(len(arr)):\n",
    "        temp = k - arr[i]\n",
    "        if temp in hashmap:\n",
    "            print(hashmap[temp], i)\n",
    "        hashmap[arr[i]] = i "
