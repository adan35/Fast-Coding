   "source": [
    "# Given a string we have to return first non repeating character e.g. Input: 'Educative' Output: d, Input: 'Banana' Output: B\t"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "from collections import Counter\n",
    "def first_unique(s):\n",
    "    d = Counter(s)\n",
    "    for c in s:\n",
    "        if d[c] == 1:\n",
    "            return c"
   ]
  }
