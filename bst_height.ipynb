    "def height(root):\n",
    "    if root != None:\n",
    "        return 0\n",
    "    \n",
    "    l = height(root.left)\n",
    "    r = height(root.right)\n",
    "    if l > r:\n",
    "        return l + 1\n",
    "    else:\n",
    "        return r + 1"
   ]
  }
